{
  "name": "standlog-analytics",
  "version": "1.0.0-mvp",
  "description": "Website Analytics MVP - Interactive heatmaps, funnels, personas, real-time dashboard, and integrations",
  "main": "js/script.js",
  "scripts": {
    "minify:jsFolder": "uglifyjs-folder ./js -eo ./standlog.min.js",
    "minify:jsFolderOne": "uglifyjs-folder ./js -o ./standlog.min.js",
    "clean:dist": "rm -rf dist",
    "rename:dist": "mv standlog.min.js dist",
